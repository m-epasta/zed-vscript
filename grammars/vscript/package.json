{
  "name": "tree-sitter-vscript",
  "version": "0.1.0",
  "description": "VScript grammar for tree-sitter",
  "scripts": {
    "build": "tree-sitter generate",
    "build-wasm": "tree-sitter build-wasm",
    "test": "tree-sitter test",
    "start": "tree-sitter playground"
  },
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "vscript"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "queries/*",
    "src/**"
  ],
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "author": "VScript Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vscript/tree-sitter-vscript.git"
  },
  "bugs": {
    "url": "https://github.com/vscript/tree-sitter-vscript/issues"
  },
  "homepage": "https://github.com/vscript/tree-sitter-vscript#readme",
  "tree-sitter": [
    {
      "scope": "source.vscript",
      "file-types": [
        "vs"
      ],
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm"
    }
  ],
  "main": "bindings/node"
}
